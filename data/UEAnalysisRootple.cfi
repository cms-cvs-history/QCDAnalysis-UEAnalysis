    #label of standard Calo Jet 
    untracked InputTag RecoCaloJetCollectionName = iterativeCone5CaloJets

    #label of charged MC particles
    untracked InputTag ChgGenPartCollectionName = chargeParticles

    #label of selected tracks
    untracked InputTag TracksCollectionName = goodTracks

    #label of trigger results
    InputTag triggerResults  = TriggerResults
    
    # label of trigger event
